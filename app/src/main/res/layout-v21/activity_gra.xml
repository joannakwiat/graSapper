        app:layout_constraintEnd_toStartOf="@+id/przycisk9"
        app:layout_constraintTop_toBottomOf="@+id/subtract" />

    <View
        android:id="@+id/przycisk9"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/ic_przycisk"
        app:layout_constraintEnd_toStartOf="@+id/przycisk7"
        app:layout_constraintTop_toBottomOf="@+id/subtract" />

    <View
        android:id="@+id/nozyczki"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_cut"
