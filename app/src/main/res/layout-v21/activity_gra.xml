        app:layout_constraintBottom_toBottomOf="@+id/przycisk11"
        app:layout_constraintEnd_toEndOf="@+id/przycisk11"
        app:layout_constraintStart_toStartOf="@+id/przycisk11"
        app:layout_constraintTop_toTopOf="@+id/przycisk11" />

