        app:layout_constraintBottom_toBottomOf="@+id/przycisk10"
        app:layout_constraintEnd_toEndOf="@+id/przycisk10"
        app:layout_constraintStart_toStartOf="@+id/przycisk10"
        app:layout_constraintTop_toTopOf="@+id/przycisk10" />

